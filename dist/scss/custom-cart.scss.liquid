.checkout_cell1,
.checkout_cell2,
.checkout_cell3,
.checkout_cell4,
.checkout_cell5,
.checkout_cell6{
	float: left;
	width: 16%;
}

.checkout_cell1 {
	width: 20%;
}

.checkout_cell2 {
	width: 32%;
}

.checkout_cell3{
	width: 7%;
}

.checkout_cell4 {
	width: 17.5%;
}

.checkout_cell5 {
	width: 17.5%;
}

.checkout_cell6 {
	width: 6%;
}



.checkout_buy_wrap{
	margin: 0 0 50px 0;
}

.checkout_titles {
	.checkout_cell2,
	.checkout_cell3,
	.checkout_cell4,
	.checkout_cell5,
	.checkout_cell6 {
		padding: 0 5px;
	}
	border-bottom: 1px $sosLightGrey solid;

	p {
		margin: 0 0 4px;
	}
}

.checkout_cell3,
.checkout_cell4,
.checkout_cell5,
.checkout_cell6{
	text-align: center;
}

.checkout_main{


	h2{
		margin: 15px 0 30px 0;
	}


	h3 {
		margin: 40px 0 25px 0;
	}


	h4{
		margin: 60px 0 20px 0;
		padding: 8px 0;	
		border-bottom: $sosLightGrey 1px solid;
	}
}


#cart-wrap {
	.cart-list{
		.checkout_buy{
			min-height: 30px;
			position: relative;
			padding: 10px;

			.checkout_cell1,
			.checkout_cell2,
			.checkout_cell3,
			.checkout_cell4 { 
				padding: 0;
				margin: 0;
			}

			.checkout_cell1{
				width: 25%;
			}

			.checkout_cell2{
				width: 50%;
				padding: 0 10px 0 0;
			}
			
			.checkout_cell3{
				text-align: left;
				width: 20%;
				padding: 0 10px 0 0;
			}

			.checkout_cell4{
				float: right;
				text-align: right;
				width: 4%;
				margin: 0;
				padding:0;
			}
		}
	}

	#checkout{
		margin:auto;
	}
	.details{
		margin: 25px 0 0 0;
		.time{
		}
		.shipping{
		}
		.returns{
		}
	}

	.cart-buttons{
		text-align:center;
	}

	.prescription-doctor,
	.prescription-address,
	.prescription-upload{
		margin: 10px 0 20px 10px;
		min-height: 100px;
	}


	.radioOptions{
		float: left;
		width: 100%;
	}

	.radioWrap{
		float: left;
		margin: 0 0 30px 10px;
		width: 100%; 
		display: inline-block;
		width: 30%;
		border:  $sosLightGrey 1px solid;
		min-height: 190px;

		.radioTitle{
			text-align: center;
			margin: 0;
			padding: 10px;
			border-bottom:  $sosLightGrey 1px solid;
			z-index: -5;
			font-size: 14px;
			font-weight: normal;
		}

		.radioNode{
			padding: 6px;
			margin: auto;
			display: none;
		}

		.radioLabel{
			width: 100%;
			text-align: center;
			padding: 70px 10px 60px 10px;
			font-weight: 400;
			margin: -34px 0 0 0;
			z-index: 10;
		}



	}

	.checkout_topbar{
		min-height: 80px;
		width: 100%;
	}

	.checkout_breadcrumb li a{
		color: #333;
	}

	.checkout_breadcrumb{
		padding: 15px 0 0 0;
		margin: 0;

		li:first-child{
			padding: 0;
		}

		li{
			display: inline;
			padding: 0 0 0 20px;
		}
	}

	.empty{
		h5{
			text-align: center;
		}
		background-color: #f2f2f2;
		padding: 40px 20px;
	}

	.checkout_remove{
		text-decoration: none;
		color: $sosLightGrey;
	}

	.cellHide{
		visibility:hidden;
	}

	.checkout_buy,
	.checkout_try{
		display: block;
		border: 1px solid $sosLightGrey;

		h5{
			margin: 0;
			padding: 0;
		}
		p{
			margin: 0;
			padding: 2px 0 0 0;
			line-height: 1.1em;
		}
		a{
			text-decoration: none;
		}
		a:hover{
			text-decoration: none;
		}
	}
	.checkout_total{
		margin-top: 20px;
	}

	.checkout_remove {
		font-size: 20px;
	}

	.generate_empty_cells {
	}
}

.col-cart-right {
	.priceTotal {
		font-size: 20px;
		padding: 20px;
		text-align: center;
	}
}


/*  <Phone>  */

@media (max-width: $screen-xs-min) {

	.template-cart {

		#content, 
		#footer-content {
			padding: 0 10%;
		}
	}


	#cart-wrap {
		.details{
			.time{
				display: inline;
			}
			.shipping{
				display: inline;
			}
			.returns{
				display: inline;
			}
			.shipping:before,
			.returns:before{
				content:"\007C";
				margin: 0 10px 0 8px;
			}
		}

		.checkout_cell2,
		.checkout_cell3,
		.checkout_cell4,
		.checkout_cell5,
		.checkout_cell6 {
			padding: 0;
		}

		.checkout_cell2{
			padding-top: 10px;
		} 

		.checkout_cell3,
		.checkout_cell4,
		.checkout_cell5{
			text-align: left;
			padding-bottom: 8px;
		} 


		.checkout_cell1{
			width: 25%;
			float: left;
			clear: none;
		} 


		.checkout_cell1 img{

			padding: 5% 0 0 0;
		}


		.checkout_cell2{
			width: 62%;
		} 

		.checkout_cell3{
			width: 10%;
			float: left;
			clear: left;
			margin-left: 25%;
		}

		.checkout_cell4,
		.checkout_cell5{
			width: 20%;
			float: left;
			clear: none;
		}

		.checkout_cell6 {
			float: right;
			clear: none;
			width: 7%;
			margin: 0;


		}




		.checkout_titles {
			.checkout_cell2,
			.checkout_cell3,
			.checkout_cell4,
			.checkout_cell5,
			.checkout_cell6 {
			}

			display: none;
		}

		.checkout_buy,
		.checkout_try {
			margin-top: 10px;
			min-height: 60px;



		}
	}

	.col-cart-left{
		float: left;
		width: 100%;
		padding: 25px 0 25px 0;
	}

	.col-cart-right{
		float: left;
		width: 100%;
	}

}

