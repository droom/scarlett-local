   font-size: 14px;
   font-weight: normal;
 }

 .radioNode{
			// padding: 16px;
			margin: auto;
			// display: none;
			visibility: hidden;
		}

		.radioLabel{
			width: 100%;
			text-align: center;
			padding: 55px 12px 30px 12px;
			font-weight: 400;
			margin:-47px 0 0 0;
			z-index: 10;
			min-height: 160px;

			border: $sosLightGrey 1px solid;

			-webkit-box-shadow: 1px 1px 1px #e7e7e7;
			-moz-box-shadow: 1px 1px 1px #e7e7e7;
			box-shadow: 1px 1px 1px #e7e7e7;
		}



	}

	.checkout_topbar{
		min-height: 80px;
		width: 100%;
	}

	.checkout_breadcrumb li a{
		color: #333;
	}

	.checkout_breadcrumb{
		padding: 25px 0 0 0;
		margin: 0;

		li:first-child{
			padding: 0;
		}

		li{
			display: inline;
			padding: 0 0 0 20px;
		}
	}

	.empty{
		h5{
			text-align: center;
		}
		background-color: #f2f2f2;
		padding: 40px 20px;
	}

	.checkout_remove{
		text-decoration: none;
		color: $sosLightGrey;
	}

	.cellHide{
		visibility:hidden;
	}

	.checkout_buy,
	.checkout_try{
		display: block;
		border: 1px solid $sosLightGrey;

		h5{
			margin: 0;
			padding: 0;
		}
		p{
			margin: 0;
			padding: 2px 0 0 0;
			line-height: 1.1em;
		}

	}
	.checkout_total{
		margin-top: 20px;
	}

	.checkout_remove {
		font-size: 20px;
	}

	.generate_empty_cells {
	}
}

.col-cart-right {
	.priceTotal {
		font-size: 20px;
		padding: 20px;
		text-align: center;
	}
}


/*  <Phone>  */

@media (max-width: $screen-xs-min) {

	.template-cart {

		#content,
		#footer-content {
			padding: 0 10%;
		}
	}


	#cart-wrap {


		.radioOptions{

			float: left;
			width: 100%;

			.radioWrap{
				float: left;
				margin-right: 0;
				margin-bottom: 15px;
				display: inline-block;
				width: 100%;
				min-height: 100px;
			}

			.radioLabel{
				padding: 55px 25px 30px 25px;
				min-height: 90px;
			}

			div.radioWrap:last-child{
				margin-right: 0;
				margin-bottom: 0;

			}
		}


		.details{
			.time{
				display: inline;
			}
			.shipping{
				display: inline;
			}
			.returns{
				display: inline;
			}
			.shipping:before,
			.returns:before{
				content:"\007C";
				margin: 0 10px 0 8px;
			}
		}

		.checkout_cell2,
		.checkout_cell3,
		.checkout_cell4,
		.checkout_cell5,
		.checkout_cell6 {
			padding: 0;
		}

		.checkout_cell2{
			padding-top: 10px;
		}

		.checkout_cell3,
		.checkout_cell4,
		.checkout_cell5{
			text-align: left;
			padding-bottom: 8px;
		}


		.checkout_cell1{
			width: 25%;
			float: left;
			clear: none;
		}


		.checkout_cell1 img{

			padding: 5% 0 0 0;
		}


		.checkout_cell2{
			width: 62%;
		}

		.checkout_cell3{
			width: 10%;
			float: left;
			clear: left;
			margin-left: 25%;
		}

		.checkout_cell4,
		.checkout_cell5{
			width: 20%;
			float: left;
			clear: none;
		}

		.checkout_cell6 {
			float: right;
			clear: none;
			width: 7%;
			margin: 0;


		}




		.checkout_titles {
			.checkout_cell2,
			.checkout_cell3,
			.checkout_cell4,
			.checkout_cell5,
			.checkout_cell6 {
			}

			display: none;
		}

		.checkout_buy,
		.checkout_try {
			margin-top: 10px;
			min-height: 60px;



		}
	}

	.col-cart-left{
		float: left;
		width: 100%;
		padding: 25px 0 25px 0;
	}

	.col-cart-right{
		float: left;
		width: 100%;
	}

}

