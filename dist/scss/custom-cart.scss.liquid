.checkout_cell1,
.checkout_cell2,
.checkout_cell3,
.checkout_cell4,
.checkout_cell5,
.checkout_cell6{
	float: left;
	width: 16%;
}

.checkout_cell1 {
	width: 20%;
	}

.checkout_cell2 {
	width: 32%;
	}

.checkout_cell3{
	width: 7%;
	}

.checkout_cell4 {
	width: 17.5%;
	}
.checkout_cell5 {
	width: 17.5%;
	}
.checkout_cell6 {
	width:6%;
	}

.checkout_stage1{
	}
.checkout_stage2{
	}
.checkout_stage3{
	}

.checkout_buy_wrap{
	margin: 0 0 50px 0;
	}

.checkout_titles {
	.checkout_cell2,
	.checkout_cell3,
	.checkout_cell4,
	.checkout_cell5,
	.checkout_cell6 {
		padding: 0 5px;
	}
	border-bottom: 1px $sosLightGrey solid;

	p {
		margin: 0 0 4px;
	}
}

.checkout_cell3,
.checkout_cell4,
.checkout_cell5,
.checkout_cell6{
	text-align: center;
}

.checkout_main{
	h2{
		margin: 15px 0 30px 0;
	}

	h4{
		margin: 60px 0 20px 0;
		padding: 8px 0;	
		border-bottom: $sosLightGrey 1px solid;
	}
}

#cart-wrap {

	#checkout{
		margin:auto;
	}

	.details{
		margin: 25px 0 0 0;

		.time{
		}

		.shipping{
		}

		.returns{
		}
	}


	.cart-buttons{
		text-align:center;
	}


	.radioWrap{
		margin: 3px 0 0 0;
		width: 100%; 
		display: inline-block;

		.radioNode{
			padding:10px;
		}

		.radioLabel{
			padding:10px;
		}

		.Prescription:after{
			content: " I need prescription lenses for my glasses."
		}

		.Readers:after{
			content: " I need reading / magnifying lenses for my glasses."
		}

		.Non-RX:after{
			contexnt: " I need non-prescription lenses for my glasses."
		}
	}

	.checkout_topbar{
		min-height: 80px;
		width: 100%;
	}

	.checkout_breadcrumb li a{
		color: #333;
	}

	.checkout_breadcrumb{
		padding: 15px 0 0 0;
		margin: 0;

		li:first-child{
			padding: 0;
		}

		li{
			display: inline;
			padding: 0 0 0 20px;
		}
	}


	.empty{
		h5{
			text-align: center;
		}
		background-color: #f2f2f2;
		padding: 40px 20px;
	}

	.checkout_remove{
		text-decoration: none;
		color: $sosLightGrey;
	}

	.cellHide{
		visibility:hidden;
	}



	.checkout_buy,
	.checkout_try{
		display: block;
		border: 1px solid $sosLightGrey;

		h5{
			margin: 0;
			padding: 0;
		}
		p{
			margin: 0;
			padding: 2px 0 0 0;
			line-height: 1.1em;
		}
		a{
			text-decoration: none;
		}
		a:hover{
			text-decoration: none;
		}
	}
	.checkout_total{
		margin-top: 20px;
	}

	.checkout_remove {
		font-size: 20px;
	}

	.generate_empty_cells {
	}
}

.col-cart-right {
	.priceTotal {
		font-size: 20px;
		padding: 20px;
		text-align: center;
	}
}


/*  <Phone>  */

@media (max-width: $screen-xs-min) {

	.template-cart {

		#content, 
		#footer-content {
			padding: 0;
		}
	}


	#cart-wrap {
		.details{
			.time{
				display: inline;
			}
			.shipping{
				display: inline;
			}
			.returns{
				display: inline;
			}
			.shipping:before,
			.returns:before{
				content:"\007C";
				margin: 0 10px 0 8px;
			}
		}

		.checkout_cell2,
		.checkout_cell3,
		.checkout_cell4,
		.checkout_cell5,
		.checkout_cell6 {
			padding: 5px 5px;
		}

		.checkout_cell1 {
		}

		.checkout_cell2 {
		}

		.checkout_cell3{
		}

		.checkout_cell4 {
		}

		.checkout_cell5 {
		}

		.checkout_cell6 {
		}


		.checkout_titles {
			.checkout_cell2,
			.checkout_cell3,
			.checkout_cell4,
			.checkout_cell5,
			.checkout_cell6 {
			}
		}

		.checkout_buy,
		.checkout_try {
			margin-top: 10px;
			min-height: 60px;



		}
	}

	.col-cart-left{
		float: left;
		width: 100%;
		padding: 25px 0 25px 0;
	}

	.col-cart-right{
		float: left;
		width: 100%;
	}

}

