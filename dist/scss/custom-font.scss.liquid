body{
  text-rendering: optimizeLegibility;
}



h1,
h2,
h3,
h4,
h5,
h6,
p,
li,
td,
legend,
label,
small,
input,
select,
textarea,
.money,
.btn{
  font-family: 'Avenir Regular', Helvetica, Arial, sans-serif;
  color: $sosMidGrey;
}



#navbar-top,
#navbar-footer{
